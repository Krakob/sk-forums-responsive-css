{
  "manifest_version": 2,

  "name": "Mobile SK forums",
  "description": "Make SK forums mobile friendly",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["http://forums.spiralknights.com/*"],
      "css": ["sk-forums-mobile.css"],
      "js": ["insert-viewport-tag.js"]
    }
  ]
}
